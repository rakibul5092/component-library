{
  "name": "my-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "test:lib": "npx jest --config=jest.config.js --detect-open-handles --silent",
    "clear_jest": "npx jest --clearCache",
    "build_lib": "ng build nextsapien-component-lib",
    "build_lib:watch": "ng build nextsapien-component-lib --watch",
    "npm_pack": "cd dist/nextsapien-component-lib && npm pack",
    "npm_pub": "cd dist/nextsapien-component-lib && npm publish",
    "package": "npm run build_lib && npm run npm_pack",
    "bump_version": "node scripts/increment-version.js -v patch",
    "bump": "cd projects/nextsapien-component-lib && npm version patch",
    "husky:prepare": "husky install && husky set .husky/pre-commit \"npm run lint\" &&  husky set .husky/pre-push \"ng build nextsapien-component-lib && ng build nextsapien-component-lib-test\" ",
    "prepare": "is-ci || npm run husky:prepare",
    "format": "npx prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.2",
    "@angular/cdk": "^14.1.2",
    "@angular/common": "^14.1.2",
    "@angular/compiler": "^14.1.2",
    "@angular/core": "^14.1.2",
    "@angular/forms": "^14.1.2",
    "@angular/localize": "^14.1.2",
    "@angular/material": "^14.1.2",
    "@angular/material-moment-adapter": "^14.2.0",
    "@angular/platform-browser": "^14.1.2",
    "@angular/platform-browser-dynamic": "^14.1.2",
    "@angular/router": "^14.1.2",
    "@ionic/angular": "^6.2.2",
    "@ng-select/ng-select": "^9.0.2",
    "@ngrx/data": "^14.1.0",
    "@ngrx/effects": "^14.1.0",
    "@ngrx/entity": "^14.1.0",
    "@ngrx/store": "^14.1.0",
    "@ngrx/store-devtools": "^14.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@tinymce/tinymce-angular": "^7.0.0",
    "eslint-config-prettier": "^8.5.0",
    "google-libphonenumber": "^3.2.31",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ngx-bootstrap": "^9.0.0",
    "ngx-google-places-autocomplete": "^2.0.5",
    "ngx-mask": "^14.2.4",
    "ngx-socket-io": "~4.3.0",
    "primeicons": "^5.0.0",
    "primeng": "14.0.1",
    "rxjs": "^7.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular-eslint/builder": "14.0.3",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/schematics": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "^14.1.2",
    "@angular/compiler-cli": "^14.1.2",
    "@angular/language-service": "^14.1.2",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@ionic/angular-toolkit": "^7.0.0",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.18.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "ng-packagr": "^14.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.7.4"
  }
}
